//Sum of All Elements in a 2D Array!!
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int R,C,sum=0 ;
    scanf("%d%d",&R,&C);
    int A[R][C] ;
    for(int i=0;i<R;i++){
        for(int j=0;j<C;j++){
            scanf("%d",&A[i][j]);
        }
    }
    for(int i=0;i<R;i++){
        for(int j=0;j<C;j++){
            sum=sum +A[i][j] ;
            
        }
    }
    printf("%d",sum);
    
    return 0;
}
