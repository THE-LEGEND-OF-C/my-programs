#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include<ctype.h>

int main(){
    int count=0 ;
    char str[1000] ;
    fgets(str,sizeof(str),stdin) ;
    for(int i=0;str[i] !='\0';i++){
        if(isalpha(str[i])){
            str[i]=tolower(str[i]) ;
            if(str[i]=='a'||str[i]=='e'||str[i]=='i'||str[i]=='o'||str[i]=='u'){
                count++ ;
            }
        }
    }
    printf("%d",count);
    return 0;
}
