//ARRAY
#include<stdio.h>
int main(){
    int size ;
    printf("Enter size of array=");
    scanf("%d",&size);
    int A[size];
    printf("Enter elements of array:");
    for(int i=0;i<size;i++)
    {
        scanf("%d",&A[i]);
    }
    printf("ELEMENTS OF ARRAY =\n");
    for(int i=0;i<size;i++){
        printf("%d\n",A[i]);
    }




    return 0;
}
