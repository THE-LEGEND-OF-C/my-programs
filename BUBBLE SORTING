//BUBBLE SORTING
#include<stdio.h>
int main(){
    int size;
    printf("ENTER SIZE OF ARRAY = ");
    scanf("%d",&size);
    int A[size];
    printf("ENTER ELEMENTS OF ARRAY = ");
    for(int i=0;i<size;i=i+1)
    {
        scanf("%d",&A[i]);
    }
    for(int i=0;i<size-1;i++)
    {
        for(int j=0;j<size-1-i;j++)
        {
            if(A[j]>A[j+1])
            {
                int x=A[j] ;
                A[j]=A[j+1] ;
                A[j+1]=x ;
            }
        }
    }
    for(int i=0;i<size;i++)
    {
        printf("%d ",A[i]);
    }

    return 0;
}
