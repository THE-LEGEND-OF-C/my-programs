//Sum of Each Row and Column in a 2D Array!!
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int R,C,sum1,sum2;
    scanf("%d%d",&R,&C);
    int A[R][C] ;
    for(int i=0;i<R;i++){
        for(int j=0;j<C;j++){
            scanf("%d",&A[i][j]);
        }
    }
    for(int i=0;i<R;i++){
        sum1=0;
        for(int j=0;j<C;j++)
        {  
            sum1=sum1+A[i][j] ;
        }
        printf("%d\n",sum1);
    }
    for(int i=0;i<C;i++){
        sum2=0;
        for(int j=0;j<R;j++)
        {  
            sum2=sum2+A[j][i] ;
        }
        printf("%d\n",sum2);
    }
    
    
    
    return 0;
}
